<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oracle.jmAuto.Notice_TableMapper.jw">
 	<select id="NoticeListCls" resultType="Notice_Table">
 		SELECT *
		FROM Notice_Table


 	</select>
 	
 	<select id="NoticeSelOne" parameterType="int" resultType="Notice_Table">
 		SELECT *
 		FROM Notice_Table
 		WHERE notice_num = #{notice_num}
 	</select>
	
 </mapper>